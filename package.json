    "phantomjs-prebuilt": "^2.1.7",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-tools": "^0.10.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-diff-monitor": "^5.0.5",
