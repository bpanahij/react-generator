{
  "name": "react-generator",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/bpanahij/react-generator.git"
  },
  "description": "A React/Redux Starter Kit / Generator",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/babel-cli/bin/babel-node.js ./node_modules/karma/bin/karma start ./karma.conf",
    "test:dev": "npm run test -- --watch",
    "start": "NODE_ENV=development webpack-dev-server",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "build:production": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "generators:install": "sudo -S npm link utils/generators/generator-crm/",
    "generators:component": "node utils/generate_component.js",
    "generators:component:stateful": "node utils/generate_stateful_component.js",
    "generators:action": "node utils/generate_action.js",
    "generators:thunk": "node utils/generate_thunk.js",
    "generators:module": "node utils/generate_module.js",
    "generators:route": "node utils/generate_route.js",
    "json-generator:fixture": "node src/fixtures/api/fixtureGenerator.js"
  },
  "keywords": [
    "front",
    "end",
    "web"
  ],
  "author": "Brian Johnson",
  "license": "MIT",
  "dependencies": {
    "express": "4.14.0"
  },
  "devDependencies": {
    "classnames": "2.2.5",
    "draft-js": "0.8.0",
    "history": "^2.0.0",
    "immutable": "^3.8.1",
    "intl": "^1.2.4",
    "isomorphic-fetch": "^2.2.1",
    "normalizr": "2.2.1",
    "object-assign": "4.1.0",
    "react": "^15.2.1",
    "react-intl": "^2.1.3",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.3",
    "underscore": "1.8.3",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "6.11.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-enzyme": "^0.5.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.20.0",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "enzyme": "^2.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "fetch-mock": "5.1.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.29.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-intl-shim": "1.0.3",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack": "^1.7.0",
    "madoka": "1.1.2",
    "mocha": "2.5.3",
