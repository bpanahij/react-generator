    "redux-devtools-inspector": "^0.8.0",
    "redux-devtools-instrument": "^1.1.2",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "redux-slider-monitor": "^1.0.7",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "suppose": "^0.6.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "yargs": "^4.8.1",
    "yo": "^1.8.4"
  }
}
